@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Threads REQUIRED)
if(NOT @GLOBAL_HOTKEY_DISABLE_HOOK@)
    find_dependency(kbdt REQUIRED)
endif()
if(APPLE)
    find_dependency(Carbon CoreFoundation REQUIRED)
endif()
if(NOT @GLOBAL_HOTKEY_DISABLE_REGISTER@ AND LINUX)
    find_dependency(X11)
endif()

include(@PACKAGE_CMAKE_INSTALL_LIBDIR@/cmake/@PROJECT_NAME@/@PROJECT_NAME@Targets.cmake)

set_and_check(@PROJECT_NAME@_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)
set_and_check(@PROJECT_NAME@_LIBRARY_DIRS @PACKAGE_CMAKE_INSTALL_LIBDIR@)
set(@PROJECT_NAME@_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@)

set(GLOBAL_HOTKEY_VERSION @PROJECT_VERSION@)
set(GLOBAL_HOTKEY_DISABLE_HOOK @GLOBAL_HOTKEY_DISABLE_HOOK@)
set(GLOBAL_HOTKEY_DISABLE_REGISTER @GLOBAL_HOTKEY_DISABLE_REGISTER@)
set(GLOBAL_HOTKEY_SHARED @GLOBAL_HOTKEY_SHARED@)
set(@PROJECT_NAME@_FOUND TRUE)
check_required_components(@PROJECT_NAME@)
